<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <Button
        android:id="@+id/test_button1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="connCallbackAIDL"
        android:text="test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <Button
        android:id="@+id/test_button2"
        app:layout_constraintTop_toBottomOf="@+id/test_button1"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="testCallbackAIDL"
        android:text="speak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <Button
        android:id="@+id/test_button3"
        app:layout_constraintTop_toBottomOf="@+id/test_button2"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="connUserAIDL"
        android:text="connect_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/test_button4"
        app:layout_constraintTop_toBottomOf="@+id/test_button3"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="testUserAIDL"
        android:text="use_userinfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>